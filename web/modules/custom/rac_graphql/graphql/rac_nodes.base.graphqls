# Nodes.
########################################

interface NodeInterface {
  id: ID!
  title: String!
}

type NodeLandingPage implements NodeInterface {
  id: ID!
  title: String!
  author: String
}

type NodeJob implements NodeInterface {
  id: ID!
  title: String!
  jobTitle: String!
  company: String!
  jobPeriod: DateRange!
  description: String!
  author: String
}
